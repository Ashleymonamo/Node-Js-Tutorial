                  <th>Action</th>
                </tr>
        
        <tbody id="sub">
            <tr>
                <th scope="row">1</th>
                <td >ID</td>
                <td> <input type="file" id="ID"><br> 
                    <progress id="bar" max="100" value="0">
                    0%
                   </progress><br>
                   <span id="percentage"></span></td>
                <td> <button onclick="uploadID()">Upload</button> </td>
                
              </tr> 
              <tr>
                <th scope="row">2</th>
                <td>Recent Results</td>
                <td> <input type="file" id="results"><br>
                    <progress id="bar2" max="100" value="0">
                        0%
                       </progress><br>
                       <span id="percentage2"></span></td>
                <td><button onclick="uploadResults()">Upload</button> </td>
                
              </tr> 
        </tbody>
                
                </table>

           
           
         
            </div>

            
         </div>

     


    
 


